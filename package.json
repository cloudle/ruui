{
  "name": "react-universal-ui",
  "description": "Universal UI Kit for React Native and Web",
  "main": "dist",
  "files": [
    "dist",
    "src",
    "cli-local",
    "cli.js",
    "!**/__tests__"
  ],
  "version": "0.9.05",
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "build": "babel ./src --out-dir ./dist",
    "bundle": "ENV=prod webpack --config webpack.config.js",
    "release": "npm run build && npm run bundle && npm publish",
    "web-vendor": "webpack --config webpack.vendor.config.js --progress",
    "web": "node webpack.devserver",
    "ios": "react-native run-ios",
    "android": "react-native run-android"
  },
  "license": "MIT",
  "author": "Cloud Le",
  "repository": {
    "type": "git",
    "url": "git://github.com/cloudle/ruui.git"
  },
  "dependencies": {
    "commander": "^2.15.1",
    "lodash": "^4.17.5",
    "minimist": "^1.2.0",
    "react-native-drawer": "^2.5.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "tinycolor2": "^1.4.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "22.1.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-flow-react-proptypes": "^2.2.1",
    "babel-plugin-module-resolver": "^2.5.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.12",
    "babel-polyfill": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-es2015-loose": "^8.0.0",
    "babel-preset-react-native": "^4.0.0",
    "chalk": "^2.3.2",
    "eslint": "^4.16.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-flowtype": "^2.42.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.0",
    "file-loader": "^0.10.0",
    "html-webpack-plugin": "^2.30.1",
    "jest": "22.1.4",
    "json-loader": "^0.5.7",
    "progress-bar-webpack-plugin": "^1.10.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-hot-loader": "3.0.0-beta.7",
    "react-native": "^0.48.4",
    "react-native-drawer": "^2.3.0",
    "react-native-web": "^0.1.16",
    "react-test-renderer": "~15.4.0-rc.4",
    "universal-vector-icons": "^4.4.12",
    "url-loader": "^0.5.8",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1"
  },
  "peerDependencies": {
    "react": "^16.0.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
