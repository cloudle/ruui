{
  "name": "react-universal-ui",
  "description": "Universal UI Kit for React Native and Web",
  "main": "dist",
  "files": [
    "dist",
    "src",
    "!**/__tests__"
  ],
  "version": "0.1.29",
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "build": "babel ./src --out-dir ./dist",
    "bundle": "ENV=prod webpack --config webpack.config.js",
    "release": "npm run build && npm run bundle && npm publish",
    "web-vendor": "webpack --config webpack.vendor.config.js --progress",
    "web": "node webpack.devserver",
    "ios": "react-native run-ios",
    "android": "react-native run-android"
  },
  "license": "MIT",
  "author": "Cloud Le",
  "repository": {
    "type": "git",
    "url": "git://github.com/cloudle/ruui.git"
  },
  "dependencies": {
    "react-native-drawer": "^2.3.0",
    "react-native-web": "0.0.77",
    "react-redux": "^5.0.3",
    "redux": "^3.6.0",
    "tinycolor2": "^1.4.1"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-eslint": "^7.2.2",
    "babel-jest": "18.0.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-flow-react-proptypes": "^2.2.1",
    "babel-plugin-module-resolver": "^2.5.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.12",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-es2015-loose": "^8.0.0",
    "babel-preset-react-native": "1.9.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-flowtype": "^2.32.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "file-loader": "^0.10.0",
    "jest": "18.1.0",
    "json-loader": "^0.5.4",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-native": "0.41.1",
    "react-native-drawer": "^2.3.0",
    "react-native-slider": "^0.10.0",
    "react-native-vector-icons": "github:cloudle/react-native-vector-icons",
    "react-native-web": "0.0.77",
    "react-navigation": "github:cloudle/react-navigation",
    "react-test-renderer": "~15.4.0-rc.4",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0"
  },
  "peerDependencies": {
    "react": "^15.4.2"
  },
  "jest": {
    "preset": "react-native"
  }
}
