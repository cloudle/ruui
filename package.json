    "jest": "22.4.3",
    "json-loader": "^0.5.7",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-native": "^0.55.3",
    "react-native-drawer": "^2.5.0",
    "react-native-web": "^0.6.1",
    "react-redux": "^5.0.7",
