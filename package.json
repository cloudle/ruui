{
  "name": "react-universal-ui",
  "description": "Universal UI Kit for React Native and Web",
  "main": "dist",
  "files": [
    "dist",
    "src",
    "cli-local",
    "cli.js",
    "!**/__tests__"
  ],
  "version": "0.9.41",
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "build": "babel ./src --out-dir ./dist",
    "bundle": "ENV=prod webpack --config webpack.config.js",
    "release": "npm run build && npm run bundle && npm publish",
    "web-vendor": "webpack --config webpack.vendor.config.js --progress",
    "web": "node webpack.devserver",
    "ios": "react-native run-ios",
    "android": "react-native run-android"
  },
  "license": "MIT",
  "author": "Cloud Le",
  "repository": {
    "type": "git",
    "url": "git://github.com/cloudle/ruui.git"
  },
  "dependencies": {
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "chalk": "^2.3.2",
    "commander": "^2.15.1",
    "dotenv": "^5.0.1",
    "file-loader": "^1.1.11",
    "hard-source-webpack-plugin": "^0.6.4",
    "html-webpack-plugin": "^3.2.0",
    "lodash": "^4.17.5",
    "minimist": "^1.2.0",
    "progress-bar-webpack-plugin": "^1.11.0",
    "react-art": "^16.3.2",
    "react-hot-loader": "^4.0.1",
    "tinycolor2": "^1.4.1",
    "url-loader": "^1.0.1",
    "webpack": "^4.6.0",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-cli": "^2.0.14",
    "webpack-dev-server": "^3.1.3"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "22.4.3",
    "babel-plugin-flow-react-proptypes": "^2.2.1",
    "babel-plugin-module-resolver": "^2.5.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.12",
    "babel-preset-es2015": "^6.22.0",
